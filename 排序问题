ls = [42,0,3,0,0,1,5]  # 巨tm奇怪的排序问题，0 -> ‘NAN’；此外，同样的数排序一样，不改变位置。
idx_0 = [i for i,x in enumerate(ls) if x==0]
Set = set(ls)
LS = list(Set)
LS.remove(0)
LS.sort()
index_ls = []
for i in LS:
    index_ls.append(LS.index(i) + 1)
for i in range(len(ls)):
    if ls[i] != 0:
        ls[i] = LS.index(ls[i]) + 1
for i in idx_0:
    ls[i] = 'Nan'
print(ls)
